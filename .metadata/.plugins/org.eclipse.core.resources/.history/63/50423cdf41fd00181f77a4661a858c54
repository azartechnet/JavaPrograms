
//oops concepts
public class Demo
{
   int a=100;//class instance
  public void get1()
	{
	  String uname="niit";
		System.out.println("method-1");
	}
  public void get2()
  {
	  System.out.println("method-2");
  }


	public static void main(String[] args) 
	{
