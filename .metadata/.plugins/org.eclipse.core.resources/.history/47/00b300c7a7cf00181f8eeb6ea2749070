
public class Sample 
{
	int k=10;
	void get1()
	{
		System.out.println("Base class value is:\n"+k);
	}
}
class D extends Sample
{
	
	void get2()
	{
		int k=20;
		System.out.println("Base class value is:\n"+k);
	}
}

class Demo
{

	public static void main(String[] args) 
	{
	   D g=new D();
	   

	}

}
