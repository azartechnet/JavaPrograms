
import java.util.*;
public class Sample 
{
   int p;
   void get1()
   {
	  System.out.println("Base Class is:\n"+p);
   }
	
}
class D extends Sample
{
	  Scanner g=new Scanner(System.in);
	void get2()
	{
		//p++;
		p=g.nextInt();
		System.out.println("Dervied Class is:\n"+p);
	}
}
class Demo
{
	

	public static void main(String[] args)
	{
		D f1=new D();
		f1.get1();
		f1.get2();

	}

}
