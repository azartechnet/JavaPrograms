
interface B
{
   int a=100;
	
    void g1();
    
    
	
}
interface A extends B
{
	void g2();
}
abstract class Sample implements A
{
	public void g1()
	{
		System.out.println("Welcome to NIIT-1"+a);
	}

	@Override
	public void g2()
	{
		
		System.out.println("Welcome to NIIT-2"+a);
	}
}




public class Demo {

	public static void main(String[] args)
	{
		Sample h;
		
		h.g1();
		h.g2();

	}

}
