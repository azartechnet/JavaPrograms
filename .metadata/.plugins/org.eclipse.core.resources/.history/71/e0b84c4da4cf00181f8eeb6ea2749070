
public class Sample 
{
    int p=10;
   void get1()
   {
	  System.out.println("Base Class is:\n"+p);
   }
	
}
class D extends Sample
{
	p=11;
	void get2()
	{
		System.out.println("Dervied Class is:\n"+p);
	}
}
class Demo
{
	

	public static void main(String[] args)
	{
		D f1=new D();
		f1.get1();
		f1.get2();

	}

}
