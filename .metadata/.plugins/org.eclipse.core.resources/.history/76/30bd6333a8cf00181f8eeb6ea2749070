
public class Sample 
{
	int k1=10;
	void get1()
	{
		System.out.println("Base class value is:\n"+k1);
	}
}
class D extends Sample
{
	
	void get2()
	{
	     k1=20;
		
		
		System.out.println("Base class value is:\n"+super.k1);
	}
}

class Demo
{

	public static void main(String[] args) 
	{
	   D g=new D();
	   g.get1();
	   g.get2();

	}

}
