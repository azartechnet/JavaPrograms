
interface B
{
   int a=100;
	
    void g1();
    
    void g2();
	
}
interface A extends B
{
	System.out.println(a);
}
class Sample implements B
{
	public void g1()
	{
		System.out.println("Welcome to NIIT-1"+a);
	}

	@Override
	public void g2()
	{
		
		System.out.println("Welcome to NIIT-2");
	}
}




public class Demo {

	public static void main(String[] args)
	{
		Sample h=new Sample();
		h.g1();
		h.g2();

	}

}
