
public class Sample
{
	
	Sample()
	{
		System.out.println("welcome to default cons...");
	}
	Sample(String name)
	{
		System.out.println(name);
	}
	void get()
	{
		System.out.println("Normal function..")
	}
	
	
	

	public static void main(String[] args) 
	{
		new Sample();
		new Sample("niit");

	}

}
