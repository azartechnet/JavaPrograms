
public class Sample extends Thread
{
   public void run()
   {
	   try
	   {
		   System.out.println("ThreadRunIN");
		    
		    wait(10000);
		   
		   System.out.println("ThreadRunOut");
	   }
	   catch(InterruptedException r)
	   {
		   
	   }
   }
}

class Demo
{
	public static void main(String[] args) 
	{
		Sample t1=new Sample();

	}

}
