import java.util.HashSet;
import java.util.Iterator;

public class Demo
{

  HashSet<String>list1=new HashSet<>();	
  void get()
  {
	  list1.add("a");
	  list1.add("b");
	  list1.add("c");
	  //list1.add("c");
	  
	  //travers..
	  Iterator g1=list1.iterator();
	  
	  while(g1.hasNext())
	  {
		  System.out.println(g1.next());
	  }
	  
	  System.out.println(list1);
	  System.out.println(list1.size());
	  System.out.println(list1.remove("c"));
	  System.out.println(list1.size());
	  System.out.println(list1.contains("c"));
	  System.out.println(list1);
  }
}

class Foo
{
	public static void main(String[] args) 
	{
		
         Demo g1=new Demo();
         g1.get();
	}

}
