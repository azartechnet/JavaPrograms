
abstract class S1
{
	static int p=10;
	public void g1()
	{
		System.out.println("Welcome to Normal Method..");
	}
	abstract public void g2();
}

class D extends S1
{

	@Override
	public void g2()
	{ 
		p++;
		System.out.println("Welcome to Abstract Method is...");
		
	}
	
}



public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
