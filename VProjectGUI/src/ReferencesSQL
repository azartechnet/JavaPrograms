//Create admintable

create table adminpage(adminname varchar(100),adminpass varchar(100))

select * from adminpage

insert into adminpage values('admin','admin')

//Create addproduct table
create table addproduct(pid integer,pname varchar(100),price integer)

select * from addproduct

//Creating UserReg Table
create table userreg(uname varchar(100),pass varchar(100),gender varchar(100))

select * from userreg